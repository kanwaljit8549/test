<mvc:View controllerName="gwsp1tempproject.controller.GW_SP1_TEMP_View"
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns:tab="sap.ui.table"
    displayBlock="true"
    xmlns="sap.m">
    <Page id="page"  title="{i18n>title}">
        <content>
            <tab:Table  rows="{sp1_Model>/d/results}" id="tableId">
                <tab:columns>
                    <tab:Column id="carrIdCol">
                        <tab:label>
                            <Label id="carrIdLabel" text="Carrier Id"></Label>
                        </tab:label>
                        <tab:template>
                            <Text id="carrIdText" text="{sp1_Model>Carrid}"></Text>
                        </tab:template>
                    </tab:Column>
                    <tab:Column id="connIdCol">
                        <tab:label>
                            <Label id="connId" text="Connection Id"></Label>
                        </tab:label>
                        <tab:template>
                            <Text id="connIdText" text="{sp1_Model>Connid}"></Text>
                        </tab:template>
                    </tab:Column>
                    <tab:Column id="flightIdCol">
                        <tab:label>
                            <Label id="flightId" text="Flight Date"></Label>
                        </tab:label>
                        <tab:template>
                            <Text id="flightIdText" text="{sp1_Model>Fldate}"></Text>
                        </tab:template>
                    </tab:Column>
                    <tab:Column id="classIdCol">
                        <tab:label>
                            <Label id="classId" text="Class"></Label>
                        </tab:label>
                        <tab:template>
                            <Text id="classIdText" text="{sp1_Model>Class}"></Text>
                        </tab:template>
                    </tab:Column>
                </tab:columns>    
            </tab:Table>
            <Button id="BtnId1" text="Button"></Button>

        </content>
    </Page>
</mvc:View>
